{
    "name": "bakaphp/phalcon-api",
    "type": "project",
    "description": "Kanvas API Skeleton",
    "keywords": [
        "framework",
        "laravel",
        "mctekk",
        "kanvas"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "fruitcake/laravel-cors": "^3.0",
        "goldspecdigital/laravel-eloquent-uuid": "^9.0",
        "guzzlehttp/guzzle": "^7.0",
        "http-interop/http-factory-guzzle": "^1.2",
        "laravel/framework": "^9.41",
        "laravel/octane": "^1.2",
        "laravel/sanctum": "^3.0",
        "laravel/scout": "^9.8",
        "laravel/tinker": "^2.7",
        "lcobucci/jwt": "^4.1",
        "league/flysystem-aws-s3-v3": "^3.0",
        "meilisearch/meilisearch-php": "^0.27.0",
        "mll-lab/graphql-php-scalars": "^5.4",
        "mll-lab/laravel-graphiql": "^1.0",
        "nuwave/lighthouse": "^5.57",
        "phpclassic/php-shopify": "^1.2",
        "sentry/sentry-laravel": "^2.13",
        "shopify/shopify-api": "^4.1",
        "silber/bouncer": "^1.0",
        "spatie/data-transfer-object": "^3.7",
        "spatie/laravel-data": "^2.0",
        "symfony/http-client": "^6.1",
        "symfony/mailgun-mailer": "^6.1",
        "vladimir-yuldashev/laravel-queue-rabbitmq": "^13.0",
        "vlucas/phpdotenv": "^5.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "kitloong/laravel-migrations-generator": "^5.2",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0",
        "vimeo/psalm": "^4.22",
        "phpstan/phpstan": "^1.4.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Kanvas\\": "src/Kanvas",
            "Baka\\": "src/Baka",
            "Domains\\": "src/Domains",
            "Kanvas\\Inventory\\": "src/Inventory",
            "Kanvas\\Social\\": "src/Social",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": "vendor/bin/phpunit",
        "sail": "vendor/bin/sail",
        "php-cs-fixer": "php-cs-fixer --config=./.php-cs-fixer.php"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
